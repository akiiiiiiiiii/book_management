<h1>新規登録</h1>

<%= form_for @book, url: books_new_path do |f| %>
  <div>
    <%= f.label :title, 'タイトル' %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :content, '投稿内容' %>
    <%= f.text_area :description %>
  </div>
  <%= f.submit '登録', class: 'btn btn-info btn-block' %>
<% end %>
